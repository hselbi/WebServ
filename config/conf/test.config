server {
	server_name webserv;
	listen 127.0.0.1:4242;

	root ./www/html;

	allow_methods GET;

	autoindex on;
	index index.html index2.html index.php video.mp4;
	client_body_limit 4096;

	error_page 404 ./www/html/error/error.html;
	error_page 500 ./www/html/error/error2.html;

	location / {
		allow_methods GET;
		root ./www/html;
		
	}

	location /board {
		allow_methods GET;
		root ./www/html;
	}

	location /dir {
		allow_methods GET DELETE;
		root ./www/html;
		autoindex on;
	}
	
	location /board/content {
		allow_methods GET POST DELETE;
		root ./www/html/contents;
		index board.html;
	}

	location /cgi {
		allow_methods GET POST;
		cgi_info php /usr/bin/php;
		index index.php;
	}

	return 935 https://profile.intra.42.fr/;

}