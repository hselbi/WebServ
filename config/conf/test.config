server {
	server_name webserv2;
	listen 127.0.0.1:4242;

	root www/html;

	allow_methods GET;

	autoindex on;
	index index.html index2.html index.php;
	client_body_limit 4096;

	error_page 404 www/html/error/error.html;
	error_page 500 www/html/error/error2.html;

	location / {
		allow_methods GET;
		root www/html;
	}

	location /js-cookies {
		allow_methods GET ;
		root www/html/js-cookies;
		autoindex on;
		error_page 404 www/html/error/404_location.html;

	}

	location /sessions {
		allow_methods GET POST;
		cgi_info php /Users/adouib/Desktop/WebServ/config/cgi_binary/php-cgi;
		root www/html/sessions;
		index index.php;
	}

	location /cookies {
		allow_methods GET POST;
		cgi_info php /Users/zmahmoud/Desktop/WebServ/config/cgi_binary/php-cgi;
		root www/html/cookies;
		index index.php;
	}

	location /get {
		allow_methods GET;
		cgi_info php /Users/zmahmoud/Desktop/WebServ/config/cgi_binary/php-cgi;
		root www/html/get;
		index index.php;
	}

	location /autoindex {
		allow_methods GET;
		root www/html/autoindex;
		autoindex on;
	}

	location /autoindex/on {
		allow_methods GET;
		root www/html/autoindex/on;
		autoindex on;
	}

	location /autoindex/off {
		allow_methods GET;
		root www/html/autoindex/off;
		autoindex off;
	}

	location /static {
		allow_methods GET DELETE POST;
		root www/html/static;
		autoindex on;
	}
}
